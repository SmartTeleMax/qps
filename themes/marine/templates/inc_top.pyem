<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<title>Редакторский интерфейс "<%= brick.site.title %>"</title>
<%
styleSheetURL = getattr(brick, 'styleSheetURL', None)
if styleSheetURL:
    %><link rel="stylesheet" type="text/css" href="<%= quoteHTML(styleSheetURL) %>"><%
%>
</head>
<body>
<table width="100%">
<tr>
<td width="20%">
<%
logoURL = getattr(brick, 'logoURL', None)
if logoURL:
    %><a href="<%= brick.edPrefix+'/' %>"><img src="<%= quoteHTML(logoURL) %>" alt="Редакторский интерфейс &quot;<%= brick.site.title %>&quot;"><%
%>
</td>
<td width="80%">
<table class="topmenu" width="100%"><tr>
<%
for stream in brick.allowedStreams:
    if getattr(stream, 'showInMenu', 0):
        %><td><a href="<%= brick.edPrefix+stream.path() %>"><%= stream.title %></a></td><%
%>
</tr></table>
</td></tr>
<tr><td colspan="2" align="center">
<!-- end of inc_top -->
