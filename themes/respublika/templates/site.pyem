<script>
function toggleTable(id) {
    table = document.getElementById(id);
    rows = table.tBodies[0].rows;

    for (var i=1; i < rows.length; i++) {
        e = rows.item(i);

        if (e.style.display) {
            e.style.display = '';
        } else {
            e.style.display = 'none';
        }
    }
}
</script>

<div align="center">

</div>

</td>
<!-- /right -->
