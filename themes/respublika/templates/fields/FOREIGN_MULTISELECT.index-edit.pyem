<%
for item in stream:
    if item in value:
        %><input type="hidden" name="qps-old:<%= field_suffix %>" value="<%= item.fields['id'].convertToString(item.id) %>"><%
    %>
<nobr><input type="checkbox" name="qps-new:<%= field_suffix %>" value="<%= item.fields['id'].convertToString(item.id) %>" id="cb:<%= field_suffix %>"<%= value and ' checked' or '' %>> <%= brick.getLabel(item) %></nobr><br><%
%>
