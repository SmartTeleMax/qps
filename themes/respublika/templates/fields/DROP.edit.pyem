<span class=t11b id=db1><%= brick.title %>: </span><br><span class=s3>&nbsp;</span><br>
<span class=a12>
<select name="<%= name %>" <%= str(brick.layout) %> class=a12><%
if brick.extraOption:
    %><option value=""><%= brick.extraOption %></option><%
for item in stream:
    %><option value="<%= item.id %>"<%
    if item.id == value:
        %> selected<%
    %>><%= brick.getLabel(item) %></option><%
%>
</select>
</span>
